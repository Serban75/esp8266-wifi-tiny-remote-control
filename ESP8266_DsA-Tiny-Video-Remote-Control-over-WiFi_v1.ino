      document.getElementById("humid").innerHTML = this.responseText;
    }
  };
  xhttp.open("GET", "/humidity", true);
  xhttp.send();
}, 10000 ) ;
</script>
<script>
/* start of JSON with Ajax for POSTvars */
    $(document).ready(function(){
        
        /* start code for Light1 */
        /* change only light1button, light1icon and light1state */
        $("#light1button").click(function(){
            var current_state = $("#light1icon").hasClass("light-off");
                
            if(current_state == true)
            {
                var obj = {light1state: "On"};
                $.ajax({
                  url:"/postvars",
                  type: "POST",
                  data: JSON.stringify(obj),
                  contentType:"application/json",
                  dataType:"json",
                }); 
                $("#light1button").val("On");
                $("#light1icon").removeClass("light-off").addClass("light-on");
            }
            else
            {
                var obj = {light1state: "Off"};
                $.ajax({
                  url:"/postvars",
                  type: "POST",
                  data: JSON.stringify(obj),
                  contentType:"application/json",
                  dataType:"json",
                }); 
                $("#light1button").val("Off");
                $("#light1icon").removeClass("light-on").addClass("light-off");
            }
        });
